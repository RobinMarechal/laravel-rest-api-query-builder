{
  "name": "laravel-rest-api-query-builder",
  "version": "0.0.9
",
  "description": "JavaScript library to build RESTful API HTTP calls. This is made to work with RESTful APIs that use laravel-rest-api package. Inspired by Eloquent's Syntax. All credit goes to milroyfraser for his package sarala.",
  "main": "build/index.js",
  "scripts": {
    "build": "babel src -d build",
    "t": "jest",
    "tw": "jest --watch",
    "cc": "jest --coverage",
    "cs": "./node_modules/.bin/eslint src/*",
    "csf": "./node_modules/.bin/eslint src/* --fix"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobinMarechal/laravel-rest-api-query-builder.git"
  },
  "keywords": [
    "restful-api",
    "json-api",
    "laravel",
    "laravel-rest-api",
    "eloquent",
    "javascript",
    "orm"
  ],
  "author": "Robin Marechal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robinmarechal/laravel-rest-api-query-builder/issues"
  },
  "homepage": "https://github.com/robinmarechal/laravel-rest-api-query-builder#readme",
  "dependencies": {
    "bunch-of-exceptions": "0.0.2",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "sarala-json-api-data-formatter": "^0.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "codecov": "^3.0.0",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^22.3.0"
  }
}
